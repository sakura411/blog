<%= paginator.render do %>
  <ul class="pagination">
    <%= prev_page_tag %>
    <% each_page do |page| %>
      <% if page.left_outer? || page.right_outer? || page.inside_window? %>
        <%= page_tag page %>
      <% elsif !page.was_truncated? %>
        <%= gap_tag %>
      <% end %>
    <% end %>
    <%= next_page_tag %>
  </ul>
<% end %>
